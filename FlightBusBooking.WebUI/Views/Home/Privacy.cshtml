@model IEnumerable<FlightBusBooking.Entities.Concrete.Country>

@{
    ViewData["Title"] = "Privacy Policy";
}

<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Ad</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.CountryId</td>
            <td>@x.CountryName</td>
        </tr>
    }
</table>

